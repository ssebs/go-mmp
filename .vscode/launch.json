    "version": "0.2.0",
    "configurations": [
        {
            "name": "Go-MMP",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/main.go",
            "args": [
                "--mode",
                "NORMAL"
            ]
        },
        {
            "name": "GUI Only",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/main.go",
            "args": [
                "--gui-only"
            ]
        },
        {
            "name": "Reset Config",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/main.go",
            "args": [
                "--reset-config"
            ]
        },
        {
            "name": "test",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/tmp/test.go",
        }
    ]
}